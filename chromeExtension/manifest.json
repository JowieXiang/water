{
	"manifest_version":2,
	"name":"Internet Awareness Collector",
	"version":"2.0",
	"permissions":[
		"webRequest",
		"<all_urls>",
		"tabs"
		],
	"content_scripts":[
		{
			"matches":[
				"<all_urls>"
			],
			"js":[
				"content.js"
			]
		}
	],
	"background":{
		"scripts":["background.js"]
	},
	"browser_action":{
		"default_icon":"icon.png"
	}

}
