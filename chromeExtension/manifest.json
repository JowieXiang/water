{
	"manifest_version":2,
	"name":"Water Engine",
	"version":"1.0",
	"permissions":[
		"webRequest",
		"<all_urls>",
		"tabs"
		],
	"content_scripts":[
		{
			"matches":[
				"https://howsthewater.netlify.com/"
			],
			"js":[
				"content.js"
			]
		}
	],
	"background":{
		"scripts":["background.js"]
	},
	"browser_action":{
		"default_icon":"water128.png"
	}

}
